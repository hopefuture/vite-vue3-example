{
  "name": "vite-vue3-example",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "start": "nodemon ./dist-server/index.js",
    "watch-server": "nodemon dist-server/index.js",
    "watch-build-server": "tsc -p ./server-tsconfig.json -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node,Vue\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-build-server\" \"npm run watch-server\" \"npm run dev\"",
    "eslint": "eslint src server --fix --ext .vue,.ts,.js",
    "test": "NODE_ENV=development mocha -watch",
    "test-server": "TS_NODE_PROJECT='./server-tsconfig.json' mocha -watch"
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "element-plus": "^1.2.0-beta.5",
    "got": "^11.8.3",
    "ip": "^1.1.5",
    "koa": "^2.13.4",
    "vue": "^3.2.22",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/concurrently": "^6.4.0",
    "@types/ip": "^1.1.0",
    "@types/koa": "^2.13.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-vue": "^1.10.1",
    "@vitejs/plugin-vue-jsx": "^1.3.0",
    "@vue/compiler-sfc": "^3.2.23",
    "chai": "^4.3.4",
    "concurrently": "^6.4.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-vue": "^8.1.1",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "sass": "^1.44.0",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "unplugin-vue-components": "^0.17.2",
    "vite": "^2.6.14",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.3.0",
    "winston-transport": "^4.4.0"
  }
}
